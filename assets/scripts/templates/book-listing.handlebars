{{#each books as |book|}}
  <!-- the > symbol calls on the partial-->
  {{> partial}}
  <!-- Don't uncomment yet -->

 <!-- Developers, why are we using data-id and not just id? -->
  <h1 class='clickable' data-id="{{book.id}}"> <small>Title: {{book.title}}</small></h1>
  <div>
    <ul>
      <li class='foo'>
        {{#if book.description}}
          {{book.description}}
        {{else}}
          No description.
        {{/if}}
      </li>
    </ul>
  </div>

{{/each}}
